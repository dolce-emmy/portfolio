{
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@pages/*": ["./src/pages/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@services/*": ["./src/services/*"],
      "@assets/*": ["./src/assets/*"],
      "@config/*": ["./src/config/*"],
      "@context/*": ["./src/context/*"],
      "@constants/*": ["./src/constants/*"],
      "@types/*": ["./src/types/*"],
      "@test/*": ["./src/test/*"],
      "@mocks/*": ["./src/mocks/*"],
      "@public/*": ["./public/*"],
      "@node_modules/*": ["./node_modules/*"],
      "@root/*": ["./"],
      "@storybook/*": ["./.storybook/*"],
      
      
    }
  }
}
